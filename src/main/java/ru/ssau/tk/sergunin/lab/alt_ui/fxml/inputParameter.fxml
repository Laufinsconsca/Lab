<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="195.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="ru.ssau.tk.sergunin.lab.alt_ui.InputParameterController">
    <BorderPane prefHeight="179.0" prefWidth="320.0">
        <center>
            <VBox prefHeight="159.0" prefWidth="280.0" BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                </BorderPane.margin>
                <Label fx:id="label" prefHeight="53.0" prefWidth="283.0" text="Введите параметр функции:">
                    <VBox.margin>
                        <Insets bottom="15.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Label>
                <TextField fx:id="textField">
                    <VBox.margin>
                        <Insets top="15.0"/>
                    </VBox.margin>
                </TextField>
                <Button fx:id="button" mnemonicParsing="false" onAction="#getParameter" prefHeight="31.0"
                        prefWidth="326.0" text="Применить">
                    <VBox.margin>
                        <Insets top="30.0"/>
                    </VBox.margin>
                </Button>
            </VBox>
        </center>
    </BorderPane>
</AnchorPane>
